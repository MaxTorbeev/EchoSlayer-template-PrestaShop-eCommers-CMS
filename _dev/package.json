{
  "name": "prestashop-starter-theme-dev-tools",
  "version": "0.0.1",
  "description": "Tools to help while developing the starter theme",
  "main": "index.js",
  "scripts": {
    "dev": "node ./node_modules/cross-env/bin/cross-env.js NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules",
    "watch": "node ./node_modules/cross-env/bin/cross-env.js NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules",
    "prod": "node ./node_modules/cross-env/bin/cross-env.js NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules",
    "build": "webpack"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-loader":                 "^5.3.2",
    "bourbon":                      "5.0.0-beta.7",
    "bootstrap":                    "4.0.0-alpha.6",
    "bootstrap-touchspin":          "^3.1.1",
    "css-loader":                   "^0.23.1",
    "cross-env":                    "^3.1.4",
    "expose-loader":                "^0.7.1",
    "extract-text-webpack-plugin":  "2.0.0-rc.3",
    "file-loader":                  "^0.8.5",
    "flexibility":                  "^1.0.5",
    "jquery":                       "^2.1.4",
    "material-design-icons":        "^2.1.3",
    "node-sass":                    "^4.5.0",
    "notosans-fontface":            "~1.0.1",
    "postcss-flexibility":          "^1.0.2",
    "postcss-loader":               "^0.8.0",
    "sass-loader":                  "latest",
    "style-loader":                 "latest",
    "tether":                       "^1.1.1",
    "velocity-animate":             "^1.2.3",
    "webpack":                      "^2.x.x",
    "webpack-sources":              "^0.1.4",
    "webpack-uglify-js-plugin":     "^1.1.9"
  }
}
